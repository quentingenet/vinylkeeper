    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.14.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/leaflet": "^1.9.21",
    "chart.js": "^4.5.0",
