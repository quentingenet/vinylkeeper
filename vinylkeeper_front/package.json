    "mobile:prod:sync": "npx cap sync android",
    "mobile:prod": "npm run build:prod && npm run mobile:prod:sync && npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
